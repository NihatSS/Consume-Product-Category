@model BookVM

<div class="container mt-5">
    <h1 class="text-center">Country Detail</h1>
</div>

<div class="d-flex">
    <h1>Name:</h1>
    <h1>@Model.Name</h1>
</div>

<div class="d-flex">
    <h1>Page count:</h1>
    <h1>@Model.PageCount</h1>
</div>

<div>
    <h1>
        Authors: @foreach (var item in Model.AuthorsNames)
        {
            <span>@item,</span>
        }
    </h1>
</div>